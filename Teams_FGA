-- Charlotte Hornets total field goal attempts (not broken down by 2 & 3 pointer attempts)

DROP TABLE IF EXISTS CHA_FGA;

CREATE TABLE CHA_FGA AS
SELECT 
	Game_ID,
	team,
	game_date,
	matchup as Match_up,
	min as game_minutes,
	fga as Field_goal_attempts,
	fgm as Field_goals_made,
	(fga-fgm) as Field_goals_missed,
	fg_pct as Field_goal_Percentage
FROM games 
WHERE team = 'Charlotte Hornets';

SELECT * FROM CHA_FGA;

